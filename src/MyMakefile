all: pruebaSymbol

pruebaSymbol: symbol.o statement.o expression.o type.o pruebaSymbol.o
	g++ -std=c++0x symbol.o statement.o expression.o type.o pruebaSymbol.o -o pruebaSymbol

symbol.o: symbol.cc
	g++ -c -std=c++0x symbol.cc

statement.o: statement.cc
	g++ -c statement.cc

expression.o: expression.cc
	g++ -c expression.cc

type.o: type.cc
	g++ type.cc

pruebaSymbol.o: pruebaSymbol.c
	g++ -std=c++0x -c pruebaSymbol.c

